{
  "private": true,
  "workspaces": [
    "api",
    "front",
    "electron"
  ],
  "scripts": {
    "start:api": "cd api && npm run start:dev",
    "build:api": "cd api && npm run build",
    "start:front": "cd front && ionic serve --port 8100 --no-open",
    "build:front": "cd front && npm run build:ionic",
    "start:electron": "wait-on http://localhost:8100 && tsc -p electron && electron electron/dist/main.js",
    "build:electron": "npx tsc -p electron",
    "dev": "concurrently -k -n API,WEB,ELEC -c cyan,green,magenta \"npm run start:api\" \"npm run start:front\" \"npm run start:electron\""
  },
